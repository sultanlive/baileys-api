{
  "semi": false,
  "tabWidth": 4,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "none",
  "parser": "typescript",
  "overrides": [
    {
      "files": [
        "*.json"
      ],
      "options": {
        "singleQuote": false,
        "tabWidth": 2,
        "quoteProps": "preserve"
      }
    },
    {
      "files": [
        "*.yml"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ]
}